body{
background:#FFFFFF;
margin:9px;
font: 10pt/14pt 'Lucida Grande', Verdana, Helvetica, sans-serif;
color:#666666;
}

A:link{ color:#F39339; text-decoration:none; }
A:hover{ color:#6C8EFF; text-decoration:underline; }
A:active{ color:#F39349; text-decoration:none; }
A:active:hover{ color:#6C8EFF; text-decoration:underline; }
A:visited{ color:#F83F64; text-decoration:none; }
A:visited:hover{ color:#6C8EFF; text-decoration:underline; }


#wrap{
%width:800px;
width: 90%;
margin-left:auto;
margin-right:auto;
}

#sidebar{
float:left;
text-align:left;
#width:150px;
width:20%;
}

#container{
#width:600px;
width:75%;
%margin-left:180px;
margin-left:22%;
border-left:#CCCCCC 0px solid;
}

#content1{
width:270px;
padding:20px;
float:left;
text-align:justify;
}


#container li
{
list-style: square outside;
margin: 0;
#border-bottom:#CCCCCC 1px solid;
#text-align: right;
}

.content2{
padding:20px;
text-align:justify;
}

h3{
text-align:center;
color:#6C8EFF;
font-size:14pt;
font-weight:bold;
}

#title{
height:120px;
margin-top:30px;
border-bottom:#CCCCCC 1px solid;
text-align:right;
font-size:12pt;
letter-spacing:-1px;
color:#CCCCCC;
}

#footer{
text-align:center;
font-size:11px;
height:30px;
margin-top:10px;
border-top:#CCCCCC 1px solid;
text-transform:lowercase;
}

 #navlist
{
padding: 0 1px 1px;
margin-left: 0;
font: 11px bold 12px Verdana, sans-serif;
background:#F3F3F3;
%width: 13em;
width: 100%;
}

#navlist li
{
list-style: none;
margin: 0;
border-top:#CCCCCC 1px solid;
text-align: right;
}

#navlist li a
{
display: block;
padding: 0.25em 0.5em 0.25em 0.75em;
background: #F3F3F3;
text-decoration: none;
}

#navlist li a:link { color: #6C8EFF; }
#navlist li a:visited { color: #999999; }

#navlist li a:hover
{
border-color: #FE3;
color: #666666;
background: #FFFFFF;
}


